<div class="container">
    {{#each songs}}
    <div class="row">
        <div class="col-8">
            <h5>{{this.name}}</h5>
        </div>
        <div class="col-4">
            <audio controls>
                <source src="{{this.preview_url}}" type="audio/mp3">
            </audio>
        </div>
    </div>
    {{/each}}
</div>